{
  "name": "ie221-fullstack",
  "version": "1.0.0",
  "description": "Full-stack web application with Django backend and React frontend",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && python manage.py runserver",
    "frontend": "cd frontend && npm start",
    "install-all": "cd backend && pip install -r requirements.txt && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "test": "concurrently \"npm run test:backend\" \"npm run test:frontend\"",
    "test:backend": "cd backend && python manage.py test",
    "test:frontend": "cd frontend && npm test",
     "migrate": "cd backend && python manage.py migrate",
    "makemigrations": "cd backend && python manage.py makemigrations",
    "collectstatic": "cd backend && python manage.py collectstatic --noinput",
    "setup": "npm run install-all && npm run migrate"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "django",
    "react",
    "fullstack",
    "python",
    "javascript"
  ],
  "author": "duyle1062",
  "license": "MIT"
}
